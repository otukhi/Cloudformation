			"VpcId" : {"Ref" : "ReanVPC"},
			"Tags" : [ {"Key" : "Name", "Value" : "ReanNACL"} ]
      }
    },
	
		"InboundSSHNetworkAclEntry" : {
			"Type" : "AWS::EC2::NetworkAclEntry",
			"Properties" : {
				"NetworkAclId" : {"Ref" : "NetworkAcl"},
				"RuleNumber" : "100",
				"Protocol" : "6",
				"RuleAction" : "allow",
				"Egress" : "false",
				"CidrBlock" : "0.0.0.0/0",
				"PortRange" : {"From" : "22", "To" : "22"}
      }
    },

		"InboundHTTPNetworkAclEntry" : {
