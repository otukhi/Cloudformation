   "Resources" : {
		"ReanVPC" : {
			"Type" : "AWS::EC2::VPC",
			"Properties" : {
				"CidrBlock" : "10.0.0.0/16",
				"Tags" : [ {"Key" : "Name", "Value" : "Rean-VPC-OT"} ]
			}
		},
   

		"ReanInternetGateway" : {
			"DependsOn" : "ReanVPC",
			"Type" : "AWS::EC2::InternetGateway",
			"Properties" : {
            "Tags" : [ {"Key" : "Name", "Value" : "Rean-IGW"}]
			}
		},


		"AttachGateway" : {
			"Type" : "AWS::EC2::VPCGatewayAttachment",
			"DependsOn" : "ReanInternetGateway",
			"Properties" : {
			"VpcId" : { "Ref" : "ReanVPC" },
			"InternetGatewayId" : { "Ref" : "ReanInternetGateway" }
			}
		},
		
		
		
		"mySubnet1" : {
			"Type" : "AWS::EC2::Subnet",
			"Properties" : {
				"VpcId" : { "Ref" : "ReanVPC" },
				"CidrBlock" : "10.0.0.0/24",
				"AvailabilityZone" : "us-east-1a",
				"Tags" : [ { "Key" : "Name", "Value" : "PublicSubnet1-OT" } ]
         }
      },
	  
	  "mySubnet2" : {
			"Type" : "AWS::EC2::Subnet",
			"Properties" : {
				"VpcId" : { "Ref" : "ReanVPC" },
				"CidrBlock" : "10.0.1.0/24",
				"AvailabilityZone" : "us-east-1b",
				"Tags" : [ { "Key" : "Name", "Value" : "PublicSubnet2-OT" } ]
         }
      },
		
		
	  
	   "RouteTable" : {
			"Type" : "AWS::EC2::RouteTable",
			"Properties" : {
			"VpcId" : {"Ref" : "ReanVPC"},
			"Tags" : [ {"Key" : "Name", "Value" : "ReanRouteTable"} ]
      }
    },
	
	   "Route" : {
			"Type" : "AWS::EC2::Route",
			"DependsOn" : "AttachGateway",
			"Properties" : {
				"RouteTableId" : { "Ref" : "RouteTable" },
				"DestinationCidrBlock" : "0.0.0.0/0",
				"GatewayId" : { "Ref" : "ReanInternetGateway" }
      }
    },
	
	    "SubnetRouteTableAssociation1" : {
			"Type" : "AWS::EC2::SubnetRouteTableAssociation",
			"Properties" : {
			"SubnetId" : { "Ref" : "mySubnet1" },
			"RouteTableId" : { "Ref" : "RouteTable" }
      }
    },
	
	"SubnetRouteTableAssociation2" : {
			"Type" : "AWS::EC2::SubnetRouteTableAssociation",
			"Properties" : {
